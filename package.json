{
  "name": "codedesignplus",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "npx nx run-many",
    "affected": "npx nx show projects --affected  -- --base=remotes/origin/main~1",
    "affected:submodule": "npx nx affected --target=submodule --parallel 3",
    "affected:lint": "nx affected --target lint --parallel 3 --uncommitted --fix true",
    "affected:test": "npx nx affected --target=test --parallel 3 --ci --code-coverage",
    "affected:sonarqube": "npx nx run-many --target=sonarqube --parallel 3",
    "affected:build": "npx nx affected --target=build --parallel 3",
    "all:test": "npx nx run-many --target=test --ci --code-coverage",
    "all:sonarqube": "npx nx run-many --target=sonarqube",
    "publish": "ts-node --files ./scripts/index.ts",
    "prepare": "husky install",
    "format:check": "npx nx format:check",
    "format:write": "nx format:write"
  },
  "private": true,
  "devDependencies": {
    "@actions/core": "^1.10.1",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@commitlint/config-nx-scopes": "^17.6.4",
    "@commitlint/cz-commitlint": "^17.6.7",
    "@nx/eslint-plugin": "16.5.5",
    "@nx/jest": "16.5.5",
    "@nx/js": "16.5.5",
    "@nx/linter": "16.5.5",
    "@nx/plugin": "^16.5.5",
    "@swc-node/register": "~1.4.2",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.51",
    "@types/jest": "^29.4.0",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "commitizen": "^4.3.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "^8.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "lint-staged": "^13.2.3",
    "nx": "16.5.5",
    "nx-cloud": "latest",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.1.3",
    "sonarqube-scanner": "3.1.0"
  },
  "dependencies": {
    "@koliveira15/nx-sonarqube": "^3.1.2",
    "@codedesignplus/git-version": "0.1.0-beta.67",
    "@nx/devkit": "16.5.5",
    "@swc/helpers": "~0.5.0",
    "tslib": "^2.3.0"
  }
}
